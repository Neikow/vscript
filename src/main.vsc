struct Position {
  x: int;
  y: int;
}

struct Velocity {
  x: int;
  y: int;
}

struct Player {
  name: str;
  pos: Position;
  vel: Velocity;
}

let player = Player{
  name: 'Bob',
  pos: Position{
    x: 10,
    y: 5,
  },
  vel: Velocity{
    x: 1,
    y: 2,
  }
};

debug player;

player.pos.x = 4;

debug player;